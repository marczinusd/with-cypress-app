  },
  "devDependencies": {
    "@shelex/cypress-allure-plugin": "^2.40.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.0",
    "@types/node": "18.0.6",
    "@types/react": "18.2.8",
    "@types/react-dom": "18.0.6",
    "cross-env": "^7.0.3",
    "cypress": "12.3.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "start-server-and-test": "1.15.2",
    "typescript": "4.7.4"
