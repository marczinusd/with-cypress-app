{
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "test:tesults": "jest --config=jest.config.tesults.mjs",
    "coverage": "jest --coverage",
    "e2e": "start-server-and-test dev http://localhost:3000 \"cypress open --e2e\"",
